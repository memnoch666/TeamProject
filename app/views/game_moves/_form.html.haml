= form_for @game_move do |f|
  - if @game_move.errors.any?
    #error_explanation
      %h2= "#{pluralize(@game_move.errors.count, "error")} prohibited this game_move from being saved:"
      %ul
        - @game_move.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :game_type_id
    = f.number_field :game_type_id
  .field
    = f.label :description
    = f.text_field :description
  .field
    = f.label :code
    = f.text_field :code
  .actions
    = f.submit 'Save'
